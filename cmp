#include <cstring>
//#include <algorithm>
#include <iostream>

using namespace std;

void cmp(const char* str1, const char* str2, int &c, int n, int y){
    int i =0;
    if(y == 1){
        while(i < 10){
             if(str1[i] == str2[i]){
                c=0;
            }
            if(str1[i] > str2[i]){
                c=1;
            }
            if(str1[i] < str2[i]){
                c=-1;
            }
            i++;
        }
    }
    if(y == 2){
        while(i < n){
            if(str1[i] == str2[i]){
                c=0;
            }
            if(str1[i] > str2[i]){
                c=1;
            }
            if(str1[i] < str2[i]){
                c=-1;
            }
            i++;
    }
    }
}
int main(){
    const char str1[] = "hell world", str2[] = "hell world";
    unsigned int count, y;
    int c; 
    int n=4;
        cin >> y;
        cmp(str1, str2, c, n, y);
        cout << "\n" << c;
}
